#include <iostream>
#include <queue>
#include <Windows.h>
using namespace std;


void FillQueue(queue<int>& C, int size, queue<int>& CNext) {
    if (C.size() > size) {
        CNext.push(C.front());
        C.pop();
    }
}

int main() {
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);

    const int amountOfCommands = 5;

    queue<int> C1;
    queue<int> C2;
    queue<int> C3;
    queue<int> C4;
    queue<int> C5;

    cout << "Комнады после прохода 5ой ступени" << endl;

    for (int i = 1; i <= amountOfCommands+1; i++) {
        C1.push(i);
        C2.push(i + 1);
        C3.push(i + 2);
        C4.push(i + 3);
        C5.push(i + 4);

        cout << C1.back() << " " << C2.back() << " " << C3.back() << " " << C4.back() << " " << C5.back() << "\n";
        cin.get();
    }
}
