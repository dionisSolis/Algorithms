#include <iostream>
#include <stack>
#include <Windows.h>
using namespace std;


int main() {    
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);


    const int size = 8;

    stack<string> words;
    string sentense[size] = {"матриц.", "целочисленных", "двух", "умножения", "программу", "и", "алгоритм", "Разработать"};

    for (string word : sentense) {
        words.push(word);
    }

    while (words.empty() == false) {
        cout << words.top() << " ";
        words.pop();
    }
}   
