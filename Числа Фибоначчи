#include <iostream>
#include <Windows.h>
#include <math.h>
#include <stdio.h>
#include <ctime>
#include <iomanip>
using namespace std;

int fibonacci(int n) {
    if (n <= 1) {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);

    cout << "10ый член последовательности: " << fibonacci(10) << endl;
    cout << "15ый член последовательности: " << fibonacci(15) << endl;

    return 0;
}
